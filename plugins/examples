-- Example in C++
#include <rapidjson/document.h>
#include <iostream>
using namespace std;

int main(int argc, const char** argv) {
    rapidjson::Document data;
    data.Parse<0>(argv[1]);
    cout << "/message #Aww " << data["Type"].GetString() << endl;
    return 0;
}
-- Example in Ruby
#!/usr/bin/env ruby
require "json"
data = JSON.parse(ARGV[0])
config = JSON.parse(ARGV[1])

if data["Type"] == "PRIVMSG"
    puts "Ruby: Hello, World! :D"
end